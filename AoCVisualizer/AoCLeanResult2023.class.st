"
I am a class for JSON generated by Lean4.
"
Class {
	#name : 'AoCLeanResult2023',
	#superclass : 'AoCResult',
	#category : 'AoCVisualizer-AoC2023',
	#package : 'AoCVisualizer',
	#tag : 'AoC2023'
}

{ #category : 'accessing' }
AoCLeanResult2023 class >> baseDirectory [

    ^ (Smalltalk os environment at: 'HOME') asPath / 'Repositories/advent-of-code/Lean4/misc'
]

{ #category : 'accessing' }
AoCLeanResult2023 class >> graphTitle [

	^ self yearString , ' in Lean'
]

{ #category : 'accessing' }
AoCLeanResult2023 class >> year [

	<script: 'self new open'>
	^ 2023
]
